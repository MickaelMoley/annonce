{% extends 'base.html.twig' %}
{% block body %}
    <h1>deude</h1>
    {% if fetcher.debug == true %}
        <h1>Mode Debug</h1>
        <hr>

        Nombre d'annonces mise à jour : {{ fetcher.updatedAnnonce }}<br>
        Nombre d'annonces ajouté : {{ fetcher.updatedAnnonce }}
        Lien utilisé : <a href="{{ fetcher.link }}">{{ fetcher.link }}</a>

        <a href="{{ path('annonces') }}">Voir les annonces</a>

        {{ dump(fetcher.wish) }}
          

        {% else %}

            <h1>La base de donnée a été mise à jour.</h1>
            <hr>
            <a href="{{ path('annonces') }}">Voir les annonces</a>
     {% endif %}
{% endblock %}